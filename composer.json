{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "algolia/algoliasearch-client-php": "^1.27",
        "barryvdh/laravel-debugbar": "^3.0",
        "creativeorange/gravatar": "~1.0",
        "cviebrock/eloquent-sluggable": "^4.3",
        "devdojo/chatter": "0.2.*",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "google/recaptcha": "^1.1",
        "intervention/image": "^2.4",
        "jaybizzle/laravel-crawler-detect": "1.*",
        "jeremykenedy/laravel-exception-notifier": "^1.1",
        "jeremykenedy/laravel-https": "^1.1",
        "jeremykenedy/laravel-logger": "^1.0",
        "jeremykenedy/laravel-phpinfo": "^1.0",
        "jeremykenedy/laravel2step": "^1.0",
        "jeremykenedy/uuid": "^3.1",
        "laravel/framework": "5.5.*",
        "laravel/scout": "^6.1",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "laravelium/feed": "2.11.*",
        "laravelium/sitemap": "2.7.*",
        "laravolt/avatar": "^2.1",
        "pusher/pusher-php-server": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4",
        "socialiteproviders/generators": "^2.0",
        "socialiteproviders/google": "^3.0",
        "socialiteproviders/twitter": "^3.0",
        "tcg/voyager": "1.0.15",
        "voyager-themes": "^0.15.0",
        "wordpress-import": "^1.0",
        "yajra/laravel-disqus": "^1.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.7",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },

    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
